<% provide(:title, 'All users') %>

<h6>All users</h6>

<%= will_paginate %>

<table class="table">
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>    
<tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= render user %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= will_paginate %>
